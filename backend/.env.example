# Database Configuration
DB_URL=jdbc:postgresql://postgres:5432/moneyguard
DB_USERNAME=postgres
DB_PASSWORD=your_password_here

# JWT Configuration
JWT_SECRET=your_jwt_secret_key_here
JWT_EXPIRATION=86400000

# Redis Configuration
REDIS_HOST=redis
REDIS_PORT=6379

# Server Configuration
SERVER_PORT=8080
SPRING_PROFILES_ACTIVE=prod

# Google API Key
GOOGLE_GEMINI_API_KEY=

# VPN Proxy Configuration for AI Services
VPN_PROXY_ENABLED=true

# Multiple proxy options (comma-separated)
VPN_PROXY_URLS=

# Proxy with authentication (if needed)
VPN_PROXY_AUTH_URL=
VPN_PROXY_USER=
VPN_PROXY_PASSWORD=

# Proxy rotation settings
VPN_PROXY_ROTATION_ENABLED=true
VPN_PROXY_ROTATION_INTERVAL=120000
VPN_PROXY_CONNECTION_TIMEOUT=5000
VPN_PROXY_READ_TIMEOUT=5000

# Fallback direct connection if all proxies fail
VPN_PROXY_FALLBACK_DIRECT=true